	<xs:complexType name="FeatureRecord" sawsdl:modelReference="http://purl.org/edam/data/0002044 http://purl.org/edam/data/0000867 http://purl.org/edam/data/0001381 http://purl.org/edam/data/0000855 http://purl.org/edam/data/0002201 http://purl.org/edam/data/0000924 http://purl.org/edam/data/0000857 http://purl.org/edam/data/0000916 http://purl.org/edam/data/0002084 http://purl.org/edam/data/0000896 http://purl.org/edam/data/0002081 http://wsio.org/tripleInSawsdl?predicate=http://purl.org/edam/relation/has_format&amp;object=http://purl.org/edam/format/0002352"><!--"Sequence data"(0002044)--><!--"Sequence alignment annotation"(0000867)--><!--"Sequence alignment (pairwise)"(0001381)--><!--"Sequence annotation"(0000855)--><!--"Sequence record full"(0002201)--><!--"Sequence trace"(0000924)--><!--"Sequence database hits"(0000857)--><!--"Gene annotation"(0000916)--><!--"Nucleic acid annotation"(0002084)--><!--"Protein annotation"(0000896)--><!--"Secondary structure"(0002081)--><!--"BioXSD format"(0002352)-->
