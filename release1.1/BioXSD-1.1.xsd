	<xs:complexType name="FeatureOccurrenceData" sawsdl:modelReference="http://purl.org/edam/data/0001021 http://wsio.org/tripleInSawsdl?predicate=http://purl.org/edam/relation/has_format&amp;object=http://purl.org/edam/format/0002352">
