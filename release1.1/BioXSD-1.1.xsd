			<xs:choice maxOccurs="unbounded">
			
				<!--= Annotation by a feature (a track) =-->
				<xs:element name="annotation" sawsdl:modelReference="http://purl.org/edam/data/0003002 http://wsio.org/tripleInSawsdl?predicate=http://purl.org/edam/relation/has_format&amp;object=http://purl.org/edam/format/0002352"><!--"Sequence annotation track"(0003002)--><!--"BioXSD format"(0002352)-->
