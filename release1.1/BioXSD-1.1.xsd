								</xs:complexType>
							</xs:element>
							<xs:element name="semanticConcept" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="SemanticConcept">
											<xs:attribute name="localId" type="LocalId" use="required"/>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="method" minOccurs="0" maxOccurs="unbounded" sawsdl:modelReference="http://purl.org/edam/data/0000856">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="Method">
